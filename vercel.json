{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "functions": {
    "api/questionbank.js": {
      "maxDuration": 30
    },
    "api/apiconfig.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/exam",
      "dest": "/exam.html"
    }
  ]
}